<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".juego.GameActivity">

    <ImageView
        android:id="@+id/imageViewJeringaRoja"
        android:layout_width="114dp"
        android:layout_height="190dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vacuna_roja_png" />

    <ImageView
        android:id="@+id/imageViewJeringaVerde"
        android:layout_width="113dp"
        android:layout_height="190dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="32dp"
        android:rotation="46"
        app:layout_constraintStart_toStartOf="@+id/imageViewJeringaRoja"
        app:layout_constraintTop_toTopOf="@+id/imageViewJeringaRoja"
        app:srcCompat="@drawable/vacuna_verde_png" />
</androidx.constraintlayout.widget.ConstraintLayout>